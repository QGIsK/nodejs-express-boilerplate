{
  "name": "nodejs-express-boilerplate",
  "version": "1.0.0",
  "description": "Boiler plate for nodejs web applications",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/QGIsK/nodejs-express-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "nodejs",
    "express",
    "webapplication",
    "web",
    "api"
  ],
  "author": "Demian",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/QGIsK/nodejs-express-boilerplate/issues"
  },
  "homepage": "https://github.com/QGIsK/nodejs-express-boilerplate#readme",
  "_moduleAliases": {
    "@root": "./src",
    "@db": "./src/db/",
    "@middleware": "./src/middleware/",
    "@router": "./src/router/",
    "@utils": "./src/utils/",
    "@controllers": "./src/router/controllers",
    "@handlers": "./src/router/handlers"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.5",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.15",
    "mongoose-timestamp": "^0.6.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  }
}
